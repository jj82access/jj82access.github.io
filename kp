function clubMainAdultClick(clubid,bbscode,idx){
	if(!idx)	jQuery("a.mAdult[clubid='"+clubid+"'][bbscode='"+bbscode+"']").trigger("click");
	else		jQuery("a.mAdult[kind='view']").attr({'clubid':clubid,'bbscode':bbscode,'idx':idx}).trigger("click");
}
