<script type="text/javascript">
jQuery(function(){
  jQuery("span.sTop").on({
    'click' : function(){
      jQuery(document).scrollTop(0);
    },
    'mouseover' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon_1.png');
    },
    'mouseout' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon.png');
    }
  });

  jQuery("span.sDown").on({
    'click' : function(){
      jQuery(document).scrollTop(100000);
    },
    'mouseover' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon1_1.png');
    },
    'mouseout' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon1.png');
    }
  });

  jQuery("#bbs_perv_next_btn span.sLeft").on({
    'click' : function(){
		var code = jQuery(this).attr("code");
		var idx = jQuery(this).attr("idx");

		var url = "/ci/club/check_view_pn/1/"+idx+"/"+code;

		console.log(url);

		jQuery.ajax({
			type : "GET",
			url : url,
			dataType : "html",
			success: function(res){
				if(res != "0")	jQuery('#club_body').attr("src","/club/basic/bbs.php?clubid=amigo&idx="+res+"&bbscode="+code+"&pg_mode=view");
				else			alert("이전 글이 없습니다.");
			},
			error : function(){}
		});
    },
    'mouseover' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon2_1.png');
    },
    'mouseout' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon2.png');
    }
  });

  jQuery("#bbs_perv_next_btn span.sRight").on({
    'click' : function(){
		var code = jQuery(this).attr("code");
		var idx = jQuery(this).attr("idx");

		var url = "/ci/club/check_view_pn/2/"+idx+"/"+code;

		jQuery.ajax({
			type : "GET",
			url : url,
			dataType : "html",
			success: function(res){
				if(res != "0")	jQuery('#club_body').attr("src","/club/basic/bbs.php?clubid=amigo&idx="+res+"&bbscode="+code+"&pg_mode=view");
				else			alert("다음 글이 없습니다.");
			},
			error : function(){}
		});
    },
    'mouseover' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon3_1.png');
    },
    'mouseout' : function(){
      jQuery(this).find("img").attr('src','/images/top_icon3.png');
    }
  });

});
</script>




